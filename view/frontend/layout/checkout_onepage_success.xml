<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="searchspring.tracking.checkout.success"
                   template="Searchspring_Tracking::checkout-success-tracking.phtml" after="checkout.success">
                <arguments>
                    <argument name="searchspring_checkout_view_model"
                              xsi:type="object">Searchspring\Tracking\ViewModel\CheckoutViewModel
                    </argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
